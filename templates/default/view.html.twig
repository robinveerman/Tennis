<table class="table table-bordered">
    <tr>
        <th width="260">Ronde 1</th>
        <th>Ronde 2
            <a class="btn btn-success"
               href="{{ path('makeDuel', {'ronde': 2, 'toernooi': toernooi.id}) }}">+</a>
        </th>
        <th>Ronde 3 <a class="btn btn-success"
                       href="{{ path('makeDuel', {'ronde': 2, 'toernooi': toernooi.id}) }}">+</a></th>
        <th>Ronde 4 <a class="btn btn-success"
